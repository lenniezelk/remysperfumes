{
  "name": "remysperfumes",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "deploy": "pnpm build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface Env",
    "db:generate": "drizzle-kit generate",
    "db:local:migrate": "wrangler d1 migrations apply DB --local",
    "db:local:query": "wrangler d1 execute DB --local --command",
    "db:local:execute": "wrangler d1 execute DB --local --file",
    "seed:local:roles": "tsx src/lib/db/seed.ts --add-roles",
    "seed:local:superadmin": "tsx src/lib/db/seed.ts --add-superadmin",
    "db:prod:migrate": "wrangler d1 migrations apply DB --remote",
    "db:prod:query": "wrangler d1 execute DB --remote --command",
    "db:prod:execute": "wrangler d1 execute DB --remote --file",
    "seed:prod:roles": "tsx src/lib/db/seed.ts --add-roles --remote",
    "seed:prod:superadmin": "tsx src/lib/db/seed.ts --add-superadmin --remote",
    "kv:create": "wrangler kv namespace create --binding"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.132.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.8.3",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.17",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "daisyui": "^5.5.3",
    "drizzle-kit": "^0.31.6",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.45.3"
  }
}